{
	"name": "agentinterfacediscovery-monorepo",
	"version": "0.0.0",
	"private": true,
	"description": "A monorepo for the Agent Interface Discovery (AID) specification and tools.",
	"packageManager": "pnpm@8.15.5",
	"scripts": {
		"build": "pnpm --recursive run build",
		"dev": "pnpm --filter aid-web run dev",
		"lint": "eslint . --ext .ts,.tsx,.js,.mjs",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.mjs --fix",
		"test": "pnpm --recursive run test"
	},
	"devDependencies": {
		"@types/node": "^20.11.20",
		"typescript": "^5.3.3",
		"eslint": "^8.57.0",
		"@eslint/js": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"prettier": "^3.2.5",
		"eslint-config-prettier": "^9.1.0",
		"@aid/conformance": "workspace:*"
	},
	"keywords": [
		"ai",
		"agent",
		"discovery",
		"dns"
	],
	"author": "Agent Community",
	"license": "MIT",
	"dependencies": {
		"react-markdown": "^10.1.0",
		"remark-gfm": "^4.0.1"
	}
}