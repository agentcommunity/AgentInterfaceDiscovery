{
  "schemaVersion": "1",
  "serviceName": "Multi-Region API",
  "domain": "multi.agentcommunity.org",
  "metadata": {
    "documentation": "https://docs.agentcommunity.org/specs/aid/spec-v1/"
  },
  "implementations": [
    {
      "type": "remote",
      "name": "US-West Datacenter",
      "protocol": "mcp",
      "uri": "https://api.us-west.multi.agentcommunity.org/v1",
      "authentication": {
        "scheme": "pat",
        "description": "API Key for US-West region.",
        "placement": {
          "in": "header",
          "key": "Authorization",
          "format": "Bearer {token}"
        }
      }
    },
    {
      "type": "remote",
      "name": "EU-Central Datacenter",
      "protocol": "mcp",
      "uri": "https://api.eu-central.multi.agentcommunity.org/v1",
      "authentication": {
        "scheme": "pat",
        "description": "API Key for EU-Central region.",
        "placement": {
          "in": "header",
          "key": "Authorization",
          "format": "Bearer {token}"
        }
      }
    }
  ]
} 