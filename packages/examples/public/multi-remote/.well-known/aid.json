{
  "name": "Multi-Region API",
  "metadata": {
    "contentVersion": "2025-06-21",
    "documentation": "https://agentcommunity.org/docs/multi"
  },
  "schemaVersion": "1",
  "implementations": [
    {
      "type": "remote",
      "name": "US-West Datacenter",
      "protocol": "mcp",
      "uri": "https://api.us-west.multi.aid.agentcommunity.org/v1",
      "authentication": {
        "scheme": "pat",
        "description": "API Key for US-West region.",
        "placement": {
          "in": "header",
          "key": "Authorization",
          "format": "Bearer {token}"
        }
      }
    },
    {
      "type": "remote",
      "name": "EU-Central Datacenter",
      "protocol": "mcp",
      "uri": "https://api.eu-central.multi.aid.agentcommunity.org/v1",
      "authentication": {
        "scheme": "pat",
        "description": "API Key for EU-Central region.",
        "placement": {
          "in": "header",
          "key": "Authorization",
          "format": "Bearer {token}"
        }
      }
    }
  ]
}