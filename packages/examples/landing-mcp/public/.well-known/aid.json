{
  "name": "Agent Community Hub",
  "metadata": {
    "contentVersion": "2025-06-20",
    "documentation": "https://agentcommunity.org/docs"
  },
  "schemaVersion": "1",
  "implementations": [
    {
      "type": "remote",
      "name": "Primary Public API",
      "protocol": "mcp",
      "uri": "https://api.agentcommunity.org/v1",
      "authentication": {
        "scheme": "pat",
        "description": "API Key provided by the service.",
        "placement": {
          "in": "header",
          "key": "Authorization",
          "format": "Bearer {token}"
        }
      },
      "tags": [
        "production",
        "api"
      ]
    },
    {
      "type": "local",
      "name": "Community CLI",
      "protocol": "none",
      "package": {
        "manager": "npx",
        "identifier": "@agentcommunity/cli"
      },
      "execution": {
        "command": "npx",
        "args": [
          "-y",
          "@agentcommunity/cli"
        ]
      },
      "authentication": {
        "scheme": "none"
      },
      "tags": [
        "cli",
        "tools"
      ]
    },
    {
      "type": "remote",
      "name": "Legacy API (Deprecated)",
      "protocol": "mcp",
      "status": "deprecated",
      "uri": "https://api.agentcommunity.org/v0",
      "authentication": {
        "scheme": "none"
      },
      "tags": [
        "deprecated"
      ]
    }
  ]
}