{
    "schemaVersion": "1",
    "serviceName": "Multi-Implementation Service",
    "domain": "https://service.example.com",
    "metadata": {
      "documentation": "https://service.example.com/docs",
      "revocationURL": "https://service.example.com/revoke"
    },
    "implementations": [
      {
        "type": "remote",
        "name": "Cloud API",
        "protocol": "mcp",
        "uri": "https://api.service.example.com/v1",
        "authentication": {
          "scheme": "pat",
          "description": "Personal Access Token",
          "placement": {
            "in": "header",
            "key": "Authorization",
            "format": "Bearer {token}"
          }
        }
      },
      {
        "type": "local",
        "name": "Docker Container",
        "protocol": "mcp",
        "package": {
          "manager": "docker",
          "identifier": "service/mcp-server:latest"
        },
        "execution": {
          "command": "docker",
          "args": ["run", "-p", "8080:8080", "${package.identifier}"]
        },
        "authentication": {
          "scheme": "none"
        }
      }
    ]
  }
  