{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@agentcommunity/aid-core": ["../aid-core/src/index.ts"],
      "@agentcommunity/aid-core/*": ["../aid-core/src/*"],
      "@agentcommunity/aid-conformance": ["../aid-conformance/src/index.ts"],
      "@agentcommunity/aid-conformance/*": ["../aid-conformance/src/*"],
      "@agentcommunity/aid-core/browser": ["../aid-core/src/browser.ts"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
