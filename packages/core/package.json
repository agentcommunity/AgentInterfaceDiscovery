{
  "name": "@aid/core",
  "version": "1.0.0",
  "description": "Core library for Agent Interface Discovery (AID) types, generators, and resolvers.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "build:examples": "ts-node scripts/build-examples.ts"
  },
  "devDependencies": {
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "zod": "^3.22.4"
  }
} 