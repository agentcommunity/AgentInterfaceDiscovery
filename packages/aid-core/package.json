{
  "name": "@aid/core",
  "version": "1.0.1",
  "description": "Core library for Agent Interface Discovery (AID) types, generators, and resolvers.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "build:examples": "ts-node scripts/build-examples.ts",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "schema:generate": "ts-node ./scripts/generate-schema.ts",
    "build:schema": "pnpm run schema:generate"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "dependencies": {
    "zod": "^3.25.67"
  },
  "keywords": [],
  "author": {
    "name": "The Agent Community",
    "email": "github@agentcommunity.org",
    "url": "https://github.com/agentcommunity"
  },
  "publishConfig": {
    "access": "public"
  }
}